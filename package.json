{
  "name": "translime-plugin-rtmp-recorder",
  "version": "1.0.0",
  "description": "a plugin template",
  "license": "MIT",
  "keywords": [
    "translime",
    "translime-plugin"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite -c ui.vite.config.js",
    "build:plugin": "vite build",
    "build:ui": "vite build -c ui.vite.config.js",
    "build": "npm run build:plugin && npm run build:ui"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-vue": "7.20.0",
    "fluent-ffmpeg": "^2.1.2",
    "vite": "4.0.5",
    "vite-plugin-css-injected-by-js": "^3.0.1",
    "vue": "^3.3.0",
    "vuetify": "^3.3.3"
  },
  "plugin": {
    "title": "直播录像",
    "description": "rtmp流视频保存到文件",
    "ui": "dist/ui.esm.js"
  },
  "peerDependencies": {
    "electron": "^23.1.3",
    "vue": "^3.3.0",
    "vuetify": "^3.3.3"
  }
}
